{
  "codeLens.enable": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnType": true,
  "cSpell.languageSettings": [
    {
      "languageId": "json",
      "includeRegExpList": [ "CStyleComment" ]
    }
  ],
  "cSpell.userWords": [
    "CIDR",
    "Kubernetes",
    "VLAN",
    "WCAG",
    "ansible",
    "debounced",
    "kumihimo",
    "kumo",
    "luminanace",
    "subfolders"
  ],
  "eslint.packageManager": "yarn",
  "eslint.quiet": true
}
